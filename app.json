{
  "expo": {
    "name": "dev-radar",
    "slug": "dev-radar",

    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.imbitubadigital.devradar",
      "config": {
         "googleMaps": { "apiKey": "Sua_chave_api_google" }
      }
   }
  }
}
